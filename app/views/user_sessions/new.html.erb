<h1>Log In</h1>

<%= form_for(@session) do |f| %>
  <div class="field">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <%= f.hidden_field :remember_me, :value => false %>
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
